//  模态框中多选框样式
.checkboxcontainer{
  background-color: #f5f5f5;
  min-height: 450px;
  max-height: 450px;
  padding:20px;
  overflow-y:auto;
}
.checkboxcontainer .name{
  margin-bottom:10px;
  line-height:32px;
  border-bottom:1px solid #000000;
}
.checkbox{
  display:flex;
  flex-wrap:wrap;
}
.checkbox>label{
  margin:0px 5px 20px 0px !important;
  width:200px;
  overflow: hidden;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  white-space:nowrap;
  background-color: #ffffff;
  height: 58px !important;
  line-height: 46px !important;
}


