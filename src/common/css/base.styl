// 学员详情
.basic-wrapper {
  background: #F8F8FC;
  border-radius: 4px;
  padding: 20px;
  margin-bottom: 20px;

  .basic-title {
    border-bottom: 1px solid #DEE2EE;
    font-size: 14px;
    font-weight: bold;
    color: #1D2F4D;
    padding-bottom: 20px;
    position: relative;
    margin-bottom: 20px;

    &:before {
      content: '';
      display: inline-block;
      width: 4px;
      height: 16px;
      background: #4090FF;
      position: absolute;
      left: -20px;
    }
  }

  .el-col-12, .el-col-8 {
    height: 51px;
  }

  .el-select {
    width: 100%;
  }
  & >>> .el-form-item__label {
    position: relative;
  }
  & >>> .el-form-item.is-required:not(.is-no-asterisk)>.el-form-item__label:before {
    position: absolute;
    left: -10px;
  }
  .mini-line-height >>> .el-form-item__label {
    line-height: 20px;
  }
  .auto-label >>> .el-form-item__label {
    width: auto!important;
  }
  .opeator-btn {
    padding-left: 100px;
  }
  .set-item {
    border: 1px solid #DEE2EE;
    border-radius: 1px;
    padding: 15px 15px 0 15px;
    margin-bottom: 20px;
    .set-tips {
      border: 1px solid #FFE58F;
      border-radius: 2px;
      background: #FFFBE6;
      padding: 10px;
      color: #848477;
      font-size: 12px;
      margin-bottom: 20px;
      i {
        color: #F5B923;
        margin-right: 10px;
      }
    }
  }
}
.el-font-color-red {
  color: #F76359;
}
